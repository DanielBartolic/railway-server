# ============================================
# Railway Webhook Server - Environment Variables
# ============================================

# Supabase (same as discord-bot)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_service_role_key_here

# Stripe
STRIPE_SECRET_KEY=sk_test_your_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Credit Package Price IDs (from Stripe Dashboard)
STRIPE_PRICE_10_CREDITS=price_xxx
STRIPE_PRICE_50_CREDITS=price_xxx
STRIPE_PRICE_100_CREDITS=price_xxx

# Server (Railway sets PORT automatically)
PORT=8080
